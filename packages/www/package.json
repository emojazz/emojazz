{
  "name": "@emojazz/www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "desktop": "export IS_DESKTOP=1 && yarn build",
    "deploy": "export BASE_URL=https://emojazz.netlify.app && yarn build && cp -r ../nodejs/assets/fonts dist/  && cp -r ../nodejs/assets/img dist/ && netlify deploy"
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "jsonschema-definer": "^1.3.2",
    "minisearch": "^3.0.2",
    "preact": "^10.5.13"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.1.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/js-yaml": "^4.0.1",
    "@types/node": "^15.3.1",
    "import-sort-parser-typescript": "^6.0.0",
    "sass": "^1.32.13",
    "typescript": "^4.2.4",
    "vite": "^2.3.3"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  }
}
